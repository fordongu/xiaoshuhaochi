/*new_cart 购物车 Begin*/
.new_cart{
	position: fixed;
	bottom: 30%;
	right: 0;
	width: 400px;
	height: auto;
	overflow: hidden;
	z-index: 2;
	-webkit-transform: translateX(100%);
	-ms-transform: translateX(100%);
	-o-transform: translateX(100%);
	transform: translateX(100%); 
	-webkit-transition: all 1s ease-in-out;
	-o-transition: all 1s ease-in-out;
	transition: all 1s ease-in-out;
}
.new_cart_up{
	position: relative;
	width: 100%;
	height: auto;
	text-align: center;
}
.new_cart_up > div{
	position: relative;
	width: 100%;
	height: 40px;
	line-height: 40px;
	color: rgb(243,243,243);
	background: rgb(57,57,66);
	box-sizing:border-box;
	-webkit-sizing:border-box;
	-moz-sizing:border-box;
}
.new_cart_up > div > div{
	width: 33.33333%;
	height: 100%;
	display: inline-block;
	float: left;
	padding:0 10px;
	box-sizing:border-box;
	-webkit-sizing:border-box;
	-moz-sizing:border-box;
}
.new_cart_up .new_cart_pay div{
	float: right;
}
.new_cart_show .left_cat_gd{
	width: 100%;
	height: 100%;
	overflow: hidden;
	-moz-text-overflow: ellipsis;
	-webkit-text-overflow: ellipsis;
	-o-text-overflow: ellipsis;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.shanchu{
	position: absolute;
	top: 7px;
	left: 20px;
	width: 30%;
	height: 25px;
	line-height: 25px;
	cursor:pointer;
	background: url('/templates/newgreen/public/images/clear.png') 0 0 /25px 25px no-repeat;
	vertical-align:middle;
}
.ajaxorder{
	position: absolute;
	bottom: 0;
	right: 0;
	width: 30%;
	height: 100%;
	color: #fff;
	text-align: center;
	background: rgb(50,190,70);
	cursor:pointer;
	display: inline-block;
}
.new_cart_top,.new_cart_bottom{
	position: relative;
	width: 100%;
	color: rgb(243,243,243);
	background: rgb(57,57,66);
	height: 40px;
	line-height: 40px;
	box-sizing:border-box;
	-webkit-sizing:border-box;
	-moz-sizing:border-box;
}
.new_cart_bottom{
	text-align: left;
	color: #fff;
}
.new_cart_pic{
	position: fixed;
	bottom: 30%;
	right: 0;
	width: 30px;
	height: 35px;
	vertical-align: middle;
	cursor: pointer;
	z-index: 1;
	background: url("/templates/newgreen/public/images/shoppingcart.png") no-repeat scroll 0 0 / 30px 30px;
}
.cart_sum{
	position: absolute;
	top: 0;
	right: 3px;
	width: 20px;
	height: 20px;
	line-height: 20px;
	text-align: center;
	color: #fff;
	border-radius: 20px;
	background: #ff6000;
	display: inline-block;
}
.new_cart_logo{
	height: 40px;
	line-height: 40px;
	width:100%;
	color: #fff;
	overflow: hidden;
	cursor:pointer;
	background: rgb(37,37,47);
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.new_cart_logo_active{
	-webkit-transform: translateX(0);
	-ms-transform: translateX(0);
	-o-transform: translateX(0);
	transform: translateX(0);
}
.new_cart_logo ul{
	width: 100%;
	height: 100%;
	overflow: hidden;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.new_cart_logo ul li{
	position: relative;
	float: left;
	width: 25%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.new_cart_logo  ul li:nth-of-type(1){
	padding-left: 10px;
}
.new_cart_logo  ul li:nth-of-type(1) i{
	width: 30px;
	height: 35px;
	vertical-align: middle;
	cursor: pointer;
	background: url("/templates/newgreen/public/images/shoppingcart.png") no-repeat scroll 0 0 / 30px 30px;
}
.new_cart_logo  ul li:nth-of-type(1) span{
	position: absolute;
	left: 17%;
	width: 20px;
	height: 20px;
	line-height: 20px;
	text-align: center;
	background: #ff6000;
	color: #fff;
	border-radius: 20px;
}
.new_cart_logo big{
	color: #ff6000;
}
.new_cart_logo  ul li:last-of-type{
	text-align: right;
	padding-right: 20px;
	line-height: 50px;
}
.new_cart_logo  ul li:last-of-type i{
	width: 20px;
	height: 20px;
	cursor: pointer;
	background: url("/templates/newgreen/public/images/white_up.png") no-repeat scroll 0 0 / 20px 20px;
}
.new_cart_logo  ul li:last-of-type .active{
	background: url("/templates/newgreen/public/images/white_down.png") no-repeat scroll 0 0 / 20px 20px;
}

.new_cart_bottom big{
	color: #ff6000;
}
.goodsnum_show{
	width: 30px;
	color: rgb(243,243,243);
	background: rgb(57,57,66);
	text-align: center;
}
.left_cat_cls{
	position: absolute;
	top: 0;
	right: 10px;
	width: 40px;
	height: 40px;
	color: #aaa;
	text-align: right;
	cursor:pointer;
}
.left_cat_cls:hover{
	color: #ff6000;
}

.jian,.jia{
	width: 30px;
	height: 100%;
	font-size: 20px;
	color: rgb(50,190,70);
	cursor: pointer;
}
.jian:hover,.jia:hover{
	color: rgb(57,57,66);
	background: rgb(50,190,70);
}
/*new_cart 购物车 End*/


/* notice Begin */
.general_notice{
    position:fixed;
    top: 50%;
    left: 50%;
    width:500px;
