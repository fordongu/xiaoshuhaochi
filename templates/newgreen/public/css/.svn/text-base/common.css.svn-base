/*views_main_detail Begin*/
.views_main_detail{
    display: none;
    width: 800px;
    height: 600px; 
    position: fixed;
    top: 50%;
    left: 50%;
    margin:-300px 0 0 -400px;
    background: #fff;
    padding:10px;
    bottom: 20px;
    overflow: hidden;
    box-sizing:border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
    box-shadow: 0 0 15px #000;
    z-index: 4;
}
.views_main_detail dl{
    position: relative;
    width: 100%;
    height: 55%;
    padding-bottom:20px;
    border-bottom: 1px solid #c1c1c1;
    box-sizing:border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
}
.views_main_detail dl dt{
    width: 50%;
    height: 100%;
}
.views_main_detail dl dt ul{
    width: 100%;
    height: 100%;
}
.views_main_detail dl dt ul li:nth-child(1){
    width: 100%;
    height: 80%;
}
.views_main_detail dl dt ul li:nth-child(1) img{
    width: 390px;
    height: 240px;
    display: inline-block;
}
.views_main_detail dl dt ul li:nth-child(2){
    /*padding:14px 0;*/
    width: 100%;
    height: 5%;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.views_main_detail dl dt ul li:nth-child(2) h3{
    display: inline-block;
}
.views_main_detail dl dt ul li:nth-child(3){
     width: 100%;
    height: 19%;
}
.views_main_detail dl dt ul li:nth-child(3) img{
    width: 19%;
    height: 100%;
    display: inline-block;
}
.views_main_detail dl dt ul li:nth-child(3) img:nth-child(2){
    /*margin: 0 10px;*/
    
}
.views_main_detail dl dd{
    position: absolute;
    top: 0;
    right: 0;
    height: 100%;
    width: 48%;
    box-sizing:border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
}
.views_main_detail dl dd ul{
    position: relative;
    width: 100%;
    height: 100%;
}
.views_main_detail dl dd ul li{
    width: 100%;
    height: auto;
}
.views_main_detail dl dd ul > li:nth-child(1){
    width: 100%;
    height: 10%;
    line-height: 30px;
}
.views_main_detail dl dd ul > li:nth-child(1) h3{
    color: #242424;
    font-size: 20px;
    width: 60%;
    height: 100%;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}
.views_main_detail dl dd ul > li:nth-child(2){
    width: 100%;
    height: 10%;
}
.views_main_detail dl dd ul > li:nth-child(2) ol{
    width: 100%;
    height: 100%;
}
.views_main_detail dl dd ul > li:nth-child(2) ol li{
    width: 100%;
    height: 100%;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}
.views_main_detail dl dd ul > li:nth-child(2) ol li:nth-child(1){
    width: 50%;
    height: 100%;
    line-height: 28px;
    overflow: hidden;
}
.views_main_detail dl dd ul > li:nth-child(3){
    width: 100%;
    height: 60%;
    overflow: hidden;
}
.views_main_detail dl dd ul > li:nth-child(3) span{
    width: 100%;
    height: 100%;
    line-height: 22px;
}
.views_main_detail dl dd ul > li:nth-child(4){
    width: 100%;
    height: 20%;
    line-height: 40px;
}
.views_main_detail dl dd ul > li:nth-child(4) span{
    color: #ff6000;
    font-size: 16px;
    line-height: 60px;
}
.views_main_detail dl dd ul > li:nth-child(4) span small{
    font-size: 14px;
    margin-right: 10px;
}
.views_main_detail dl dd ul > li:nth-child(4) button{
    float: right;
    width: 100px;
    height: 32px;
    margin-top: 15px;
    line-height: 32px;
    margin-right: 10px;
    border:1px solid #242424;
}
.views_main_detail dl dd ul > li:nth-child(4) button:hover{
    background: #ff6000;
    color: #fff;
     border:1px solid #ff6000;
}
.views_main_detail div{
    position: relative;
    width: 100%;
    height: 45%;
    padding:10px 0 0;
    box-sizing:border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
}
.views_main_detail div img{
    width: 390px;
    height: 240px;
    display: inline-block;
}
.views_main_detail div > div{
    position: absolute;
    top: 0;
    left: 52%;
    width: 48%;
    height: 100%;
}
.views_main_detail div > div h5{
    width: 150%;
    height: 10%;
    line-height: 25px;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}
.views_main_detail div > div > span{
    width: 100%;
    height: 10%;
    line-height: 20px;
    display: inline-block;
}
.views_main_detail div > div > p{
    float: right;
    width: 100%;
    height: 70%;
    line-height: 22px;
    display: inline-block;
    overflow: hidden;
}
.delete{
    position: absolute;
    top: 20px;
    right: 20px;
    z-index: 2;
}
/*views_main_detail End*/



