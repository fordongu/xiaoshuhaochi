  float: left;
}
.mem_login_form ul li:nth-of-type(3) span:last-child,
.mem_login_form ul li:nth-of-type(5) a:last-child{
  float: right;
}
.mem_login_form ul li:nth-of-type(3) span:last-child a,
.mem_login_form ul li:nth-of-type(6) a{
  color: #19b4ea;
}
.mem_login_form ul li:nth-of-type(3) span:last-child a:hover,
.mem_login_form ul li:nth-of-type(6) a:hover{
  color: #f60;
  text-decoration: underline;
}
.mem_login_form ul li:nth-of-type(4){
  color: #19b4ea;
  font-size: 20px;
  border:1px solid #19b4ea;
}
.mem_login_form ul li:nth-of-type(4):hover{
  color: #fff;
  background: #19b4ea;
  cursor: pointer;
  }
.mem_login_form ul li:nth-of-type(6){
  cursor: default;
}
