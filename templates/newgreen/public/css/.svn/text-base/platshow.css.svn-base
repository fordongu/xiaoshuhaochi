.pageheader{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 40px;
	line-height: 40px;
	z-index: 9;
	text-align: center;
	font-size: 14px;
	background: #fff;
}
.pageheader ul{
	width: 100%;
	height: 100%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.pageheader ul li{
	height: 100%;
	background: #ccc;
}
.pageheader ul li a{
	width: 100%;
	height: 100%;
	color: #fff;
	display: inline-block;
}
.pageheader ul .active{
	background: #f60;
}
.pageheader ul .active a{
	color: #fff;
}


.basicinfo{margin-top:10px;background-color:#fafafa;height:150px;overflow:hidden;width:100%;}
.b-info{margin-top:-150px;margin-left:210px;}
.b-info h2 {
margin: 0 0 5px -2px;
color: #323440;
font-size: 20px;
overflow: hidden;
word-break: keep-all;
white-space: nowrap;
text-overflow: ellipsis;
width: 300px;
}
.basicinfo .b-info dl dt {
float: left;
margin-right: 5px;
