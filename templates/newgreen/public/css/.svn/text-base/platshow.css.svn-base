.COLevel em {color: #999;font-style: normal;margin: 0px 10px 0px 0px;}
.COInfo {line-height: 24px;color: #000;padding: 10px;}
.ajaxpage{width:800px;height:25px;margin-top:10px;margin-bottom:10px;}

/*翻页*/
.ajaxpage ul li{float:left;border:1px solid #CCCCCC;margin-right:2px;padding: 0 5px 0 5px;}
.ajaxpage li.a{color:#f60;text-decoration:none;}
.ajaxpage li.disabled{background-color:#ccc;padding: 0 5px 0 5px;}
.ajaxpage li.disabled a{color:#666;}
.ajaxpage li.active{background-color:#f60;color:#fff;padding: 0 5px 0 5px;}
.ajaxpage li.active a{color:#fff;}

.showshop_instr{padding:20px;}

.move_up{
	position: fixed;
	bottom: 95px;
	left: 8%;
	width: 40px;
	height: 30px;
	z-index: 999;
	cursor: pointer;
	background: url('/templates/newgreen/public/images/top.png') 0 0 / 40px 30px no-repeat;
}
.move_up span{
	position: absolute;
	top: 30px;
	left: -3px;
	color: #ff6000;
}

.pay_inshop{
	position: fixed;
	bottom: 125px;
	left: 8%;
}
#pay_prince{
	width: 40px;
	height: 40px;
	cursor: pointer;
	background: url('/templates/newgreen/public/images/pay.png') 0 0 /40px 40px no-repeat;
}
#hide_block{
	position: fixed;
	bottom: 0;
	left: 0;
	width: 100%;
	background: #fff;
	box-shadow: 0 0 10px #000;
}
#hide_block input{
	margin: 0;
	padding:0;
	border:none;
	font-size: 16px;
	border-radius: 0;
}
#hide_block input[type="text"]{
	position: absolute;
	left: 3%;
	bottom: 8%;
	width: 70%;
	height: 50%;
	padding:7px 0;
	line-height: 20px;
	text-indent: 10%;
	border: 1px solid rgba(37,37,47,.5);
}
#hide_block input[type="button"]{
	position: absolute;
	right: 0;
	bottom: 0;
	width: 25%;
	height: 100%;
	color: #fff;
	background:#f60;
}
#pay_cover{
	position: fixed;
	top: 30%;
	left: 8%;
	width: 280px;
	height: auto;
	z-index: 999;
	display: none;
	padding:20px;
	background: #fff;
	text-align: left;
	box-shadow: 0 0 10px #000;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
#pay_cover .delete{
    position: absolute;
    top: 20px;
    right: 20px;
    z-index: 2;
    width: 24px;
    height: 24px;
    cursor:pointer;
    background: url('/templates/newgreen/public/images/delete.png') 0 0 no-repeat;
}
#pay_cover li{
	width: 100%;
	height: 30px;
	line-height: 30px;
	margin: 10px 0;
	cursor: default;
}
#pay_cover li label{
	width: 100%;
	height: 100%;
	display: inline-block;
}
#pay_cover li input[type="radio"]{
	margin-right: 10px;
}
#pay_cover li:last-child button{
	width: 100%;
	height: 40px;
	line-height: 40px;
	color: #fff;
	font-size: 14px;
	background: #f60;
	border:none;
}
.goodslistimgtype .goodstypelist > ul > li dl dd .goodsbtn > li{width:30px;height: 30px;line-height: 30px;font-size: 20px;vertical-align: middle; text-align: center;border-radius: 30px;background: #f60;outline: none;}
.righ_l_b_btn_hover,
.righ_l_b_btn_moren {
position: absolute;
right: 5px;
bottom: 5px;
text-shadow:none;
outline: none;
}
