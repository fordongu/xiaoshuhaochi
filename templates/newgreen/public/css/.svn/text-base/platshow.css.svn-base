#footer { position: fixed; bottom: 0; left: 0; width: 100%; height: 50px; box-shadow: 0 0 5px #000; background: #fff; font-size: 12px; z-index: 999; text-shadow: none; overflow: hidden; }

 /*
.footer1{ height: 60px; line-height: 60px;
}
.footer2{ height: 45px; line-height: 45px;
}*/
#footer div:nth-of-type(1), #footer div:nth-of-type(2) { width: 100%; height: 100%; line-height: 50px; font-size: 13px; }

#footer div:nth-of-type(1) { line-height: 80px; font-size: 13px; }

#footer div:nth-of-type(2) span:nth-of-type(2) { float: left; width: 50%; }

#footer div:nth-of-type(2) span:nth-of-type(1), #footer div:nth-of-type(2) span:nth-of-type(3), #footer div:nth-of-type(2) span:nth-of-type(4) { position: relative; width: 25%; height: 100%; float: left; -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; }

#footer div:nth-of-type(2) span:nth-of-type(1) i { width: 45px; height: 41px; margin-top: 5px; -webkit-transform: scale(0.6); -ms-transform: scale(0.6); -o-transform: scale(0.6); transform: scale(0.6); }

#footer div:nth-of-type(2) span:nth-of-type(1) a { position: absolute; cursor: pointer; }

#footer div:nth-of-type(2) span:nth-of-type(1) { border-right: 1px solid #25252f; text-align: left; }

#footer div:nth-of-type(2) span:nth-of-type(3) { border-left: 1px solid #25252f; }

#footer div:nth-of-type(2) span:nth-of-type(1) big { position: absolute; margin: 5px 0 0 -30px; width: 20px; height: 20px; line-height: 20px; background: #ff6000; color: #fff; text-align: center; border-radius: 20px; }

#footer div:nth-of-type(2) span:nth-of-type(4) { height: 100%; background: #ff6000; color: #fff; font-size: 14px; cursor: pointer; }

#footer a { position: relative; width: 33.33333333%; height: 100%; float: left; -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; }

#footer a i { position: absolute; width: 45px; height: 41px; padding-left: 40px; margin-top: -5px; -webkit-transform: scale(0.6); -ms-transform: scale(0.6); -o-transform: scale(0.6); transform: scale(0.6); }

#footer a:nth-of-type(3) i { margin-left: -10px; }

#footer a:hover { color: #32be46; }

#footer .a span { position: absolute; width: 20px; height: 20px; line-height: 20px; border-radius: 20px; color: #fff; text-align: center; margin-left: -28px; background: #ff6000; }

