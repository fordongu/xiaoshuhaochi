@charset "utf-8";
html, body, div, p, ul, li, dl, dt, dd, h1, h2, h3, h4, h5, h6, form, input, select, button, textarea, iframe, table, th, td { margin: 0; padding:0;}
div,dl,dt,dd,form,h1,h2,h3,h4,h5,h6,img,ol,ul,li,table,th,td,p,span,a{border:0;font-weight: normal;text-decoration: none;}
article,aside,dialog,figure,footer,header,legend,nav,section{display: block;}
img,input{border:none;vertical-align:middle;}
input[type="radio"]{-webkit-appearance:normal;outline:none;}
input[type="button"],input[type="text"],input[type="password"]{-webkit-appearance: none;outline: none;background: transparent;}
input[type="button"]{text-indent: 0;}
input[type="text"],input[type="password"]{text-indent: 10px;}
input[type='number']{-moz-appearance:textfield;-webkit-appearance:textfield;appearance:textfield;}/*去除输入框右侧的加减按钮*/
input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button {-webkit-appearance: none;margin: 0;}
body{font-family:"Microsoft YaHei","微软雅黑",Tahoma,Arial,Helvetica,"宋体";font-size:12px;text-align:center;-moz-user-select:none;-khtml-user-select:none;-webkit-font-smoothing:antialiaed;}
html{overflow-y:scroll;}
ul,ol{list-style-type:none;}
th,td,input{font-size:12px;}
button{border:none;cursor:pointer;font-size:12px;background-color:transparent;-webkit-appearance: none;outline: none;}
select{border-width:1px;_zoom:1;border-style:solid;padding-top:2px;font-size:12px;}
.fl{ float:left;}
.fr{ float:right;}
.cl{ clear:both;}

ul,li{list-style:none;}

