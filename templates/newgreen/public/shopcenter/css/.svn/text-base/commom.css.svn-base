.header,.top{position: relative; height:70px;line-height: 70px; background:#45423f;width: 1240px;margin: 0 auto;}
/*.top{ height:100%; width:100%;  position:relative;}*/
.topLeft,.topRight{width:40%; height:100%;}
.top ul li a{width: 100%;height: 100%;display: inline-block; color:#c7c7c7;}
.topLeft ul{width: 100%;height: 100%;}
.topLeft ul li { cursor:pointer; float:left; font-size:14px; width:33.3333333%; text-align:center; color:#c7c7c7; font-family:"微软雅黑";}
.topLeft ul li:hover{background: #27a9e3;}
.topLeft ul li:hover a{color: #fff;}
.topRight .cost{font-size:14px; height:70px; line-height:70px; text-align:center; color:#ec7501; margin-right:26px; }
.topRight .curbtn{cursor: pointer; width: 33.333333%;height: 100%;display: inline-block;text-align: center;float: left;color: #c7c7c7;font-size: 14px;}
.topRight .curbtn:hover{background:#27a9e3;color: #fff;}
.cost img,.username img{margin-left:11px; margin-bottom:2px;}
.username{font-size:14px;color:#2f90bb;}
.shangjiaTop{ background:url(../images/shangjiaTop.png) no-repeat; position:absolute; top:0;  left:50%; margin-left:-125px; width:230px; height:66px; padding:15px 10px;}
