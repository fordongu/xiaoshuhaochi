.ico {

/* background: url(../images/icos/editor.png) right 7px no-repeat; */

background-color: transparent;

background-size: 50%;

right: 20px;

border-radius: 5px;

border: 1px solid #ea5413;

}

.container section {

padding: 2.5%;

background: #f5f5f5;

}

.address-list {
	width: 100%;
	height: 100%;
}

.address-list li {

min-height: 80px;

/* overflow: hidden; */

padding-top: 5px;

padding-left: 50px;

position: relative;

border: 1px solid #e5e5e5;

padding-left: 50px;
background: #fff;
margin: 10px 0;
/* width: 60px; */

/* height: 60px; */

/* padding: 2px; */

/* border: 1px solid $linecolor; */

}

.address-list li .count {
position: absolute;
left: 0;
top: 0;
width: 40px;
height: 100%;
font-size: 18px;
line-height: 80px;
color: #fff;
text-align: center;
background-color:#ff6000;
} 

div .adsr {
width: 100%;
display: -webkit-box;

display: -moz-box;

display: -o-box;

display: box;

}

div .addresse {

-webkit-box-flex: 1;

-moz-box-flex: 1;

-o-box-flex: 1;

box-flex: 1;

}

.address-list li .del {

width: 60px;

height: 20px;

display: none;

border: 1px solid #ff6000;

border-radius: 5px;

color: #ff6000;

cursor:pointer;
text-align: center;

margin-right: 10px;

margin-left: 10px;

}
.address-list li .del:hover{
	color: #fff;
	background: #ff6000;
}
.address-list li .del.show {

display: block;

}

